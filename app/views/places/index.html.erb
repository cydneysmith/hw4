<div class = "ps-2 pe-2">
  <div class = "pt-2 pb-2">
  <h1>Places</h1>
  </div>

  <p>
    <a class = "btn btn-dark" href="/places/new" role = "button">New Place </a>
  </p>
  <ul>
    <% for place in @places %>
      <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
    <% end %>
  </ul>
</div>

